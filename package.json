{
  "name": "naamapp",
  "description": "Official Naamat festival app",
  "author": "Anssi Herranen <anssi.herranen@iki.fi> (https://www.linkedin.com/in/anssiherranen/)",
  "license": "BSD-3-Clause",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "build:debug": "(cd android; ./gradlew assembleDebug)",
    "build:release": "(cd android; ./gradlew assembleRelease)",
    "clearcache": "watchman watch-del-all; rm -rf node_modules; yarn; rm -rf /tmp/metro-bundler-cache-*; rm -rf /tmp/haste-map-react-native-packager-*",
    "lint": "tslint src/**/*.ts{,x}",
    "lint:fix": "tslint --fix src/**/*.ts{,x}",
    "run:android": "REACT_DEBUGGER=react-native-debugger react-native run-android",
    "run:android:release": "REACT_DEBUGGER=react-native-debugger react-native run-android --variant=release",
    "server": "concurrently \"emulator @$EMULATOR\" \"yarn start\" \"react-native-debugger\"",
    "shake": "adb shell input keyevent 82",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.5.0",
    "@react-native-community/netinfo": "^3.2.1",
    "react": "16.8.3",
    "react-native": "0.59.10",
    "react-native-config": "^0.11.7",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-markdown-renderer": "^3.2.8",
    "react-native-onesignal": "^3.2.14",
    "react-navigation": "^3.11.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-native": "^0.57.65",
    "@types/react-redux": "^7.1.1",
    "@types/react-test-renderer": "^16.8.2",
    "babel-jest": "^24.8.0",
    "concurrently": "^4.1.1",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "react-test-renderer": "16.8.3",
    "redux-devtools-extension": "^2.13.8",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
